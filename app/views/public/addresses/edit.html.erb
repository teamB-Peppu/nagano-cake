<div class="container">
  <div class="row">
    <dic class="col-">
      <h3>配送先編集</h3>
      <%= do || %>
        <table>
          <tr>
            <th><%= f.label :'郵便番号(ハイフンなし)' %></th>
            <td><%= f.text_field :%></td>
          </tr>
          <tr>
            <th><%= f.label :'住所' %></th>
            <td><%= f.text_field :%></td>
          </tr>
          <tr>
            <th><%= f.label :'宛名' %></th>
            <td><%= f.text_field :%></td>
            <td><%= f.submit "変更内容を保存", class:"btn btn-sm btn-success" %></td>
          </tr>
        </table>
      <% end 
    </dic>
  </div>
</div>